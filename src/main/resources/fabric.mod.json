{
  "schemaVersion": 1,
  "id": "communityleaders",
  "version": "${version}",
  "name": "CommunityLeaders",
  "description": "Mod allowing streamers to manage their communities with custom whitelist invitations",
  "authors": ["DrEmixam"],
  "contact": {},
  "license": "AGPL-3.0",
  "icon": "community-leaders.png",
  "environment": "*",
  "entrypoints": {
    "fabric-datagen": [
      "com.dremixam.communityleaders.client.CommunityleadersDataGenerator"
    ],
    "client": [
      "com.dremixam.communityleaders.client.CommunityleadersClient"
    ],
    "main": [
      "com.dremixam.communityleaders.Communityleaders"
    ]
  },
  "mixins": [
    "communityleaders.mixins.json",
    {
      "config": "communityleaders.client.mixins.json",
      "environment": "client"
    }
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": "*",
    "minecraft": "${minecraft_version}",
    "luckperms": "*"
  },
  "suggests": {
  }
}
